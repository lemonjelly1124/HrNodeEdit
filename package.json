{
  "name": "hr-nodeeditor",
  "version": "1.0.0",
  "description": "节点编辑器",
  "main": "index.js",
  "scripts": {
    "dev": "run-p dev:view-*",
    "dev:extension": "pnpm run -F hr-nodeeditor watch",
    "dev:view-react": "pnpm run -F view-react watch",
    "dev:view-reactside1": "pnpm run -F view-reactside1 watch",
    "clear": "run-s clear:* && rm -rf ./node_modules",
    "clear:extension": "pnpm run -F hr-nodeeditor clear",
    "clear:view-react": "pnpm run -F view-react clear",
    "clear:view-reactside1": "pnpm run -F view-reactside1 clear",
    "build": "run-s build:*",
    "build:view-react": "pnpm run -F view-react build",
    "build:view-reactside1": "pnpm run -F view-reactside1 build",
    "build:extension": "pnpm run -F hr-nodeeditor compile",
    "package": "pnpm run -F hr-nodeeditor package"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.4",
    "cec-client-server": "^1.3.1",
    "path-browserify": "^1.0.1"
  },
  "devDependencies": {
    "@types/path-browserify": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5"
  }
}